<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="HTML" injector-id="js">
      <display-name>HTML in JS strings</display-name>
      <place disabled="true"><![CDATA[isHtmlLikeText()]]></place>
    </injection>
    <injection language="JSX Harmony" injector-id="js">
      <display-name>React JSX in JS strings</display-name>
      <place disabled="true"><![CDATA[isReactJSXLikeText()]]></place>
    </injection>
    <injection language="CSS" injector-id="xml">
      <display-name>body</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("body")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>